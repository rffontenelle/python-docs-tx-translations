msgstr "``PY_MINOR_VERSION`` (``4`` dalam ``3.4.1a2``)"

#: ../../c-api/apiabiversion.rst:24
msgid "``3``"
msgstr "``3``"

#: ../../c-api/apiabiversion.rst:24
msgid "``17-24``"
msgstr "``17-24``"

