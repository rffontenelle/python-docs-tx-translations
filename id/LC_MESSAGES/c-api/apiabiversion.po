msgid "``PY_MAJOR_VERSION`` (the ``3`` in ``3.4.1a2``)"
msgstr "``PY_MAJOR_VERSION`` (``3`` dalam ``3.4.1a2``)"

#: ../../c-api/apiabiversion.rst:21
msgid "``2``"
msgstr "``2``"

#: ../../c-api/apiabiversion.rst:21
msgid "``9-16``"
msgstr "``9-16``"

